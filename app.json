{
  "expo": {
    "name": "Thrift",
    "slug": "Thrift",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./app/assets/logo-square.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./app/assets/Thrift-Splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/logo-square.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": ".app/assets/logo.png"
    }
  }
}
